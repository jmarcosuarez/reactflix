{
  "name": "minimal-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev-api\" \"npm run webpack-dev-server\"",
    "webpack-dev-server": "webpack-dev-server --env=dev --progress --colors --inline",
    "dev-api": "node ./src/app/server/api.js",
    "build": "npm run clean && webpack -p --env=prod --progress --profile --colors",
    "prod": "node server.prod.js",
    "clean": "rimraf ./dist/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
    "repository": {
    "type": "git",
    "url": "https://github.com/jmarcosuarez/reactflix.git"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.23.1",
