{
  parser: "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    "class-methods-use-this": 0,
    "no-trailing-spaces": 0,
    "max-len": [1, 120, 2, {ignoreComments: true}],
    "react/prop-types": [2],
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "react/forbid-prop-types": 0,
  }
}
